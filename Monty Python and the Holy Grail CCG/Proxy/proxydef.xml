<?xml version="1.0" encoding="utf-8" ?>
<templates>
    <blocks>
             
        <block type="text" id="Title">
          <location x="50" y="30" />
          <text color="#000000" size="24" />
          <border color="#000000" size="1" />
          <wordwrap height="60" width="485" shrinktofit="True" />
        </block>
 
		<block type="overlay" id="over" src="Images/damage.png">
		  <location x="0" y="0" />
		</block>
 
    </blocks>

    <template src="Images/proxy.jpg" default="True">
    <matches>
      <match name="Type" value="Group" />
    </matches>
	
	<overlayblocks>
		<conditional>
			<switch property="name">
				<case value="XXXXXXXXX">
					<link block="over" />
				</case>
			</switch>
		</conditional>
	</overlayblocks>
    <textblocks>
      <link block="Title"><property name="name" /></link>
    </textblocks>
    </template> 
</templates>
